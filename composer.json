{
    "name": "reyadussalahin/hello-world",
    "type": "project",
    "description": "The HelloWorld Framework",
    "keywords": [
        "framework",
        "hello-world"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Md. Reyadus Salahin",
            "email": "reyadussalahin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "reyadussalahin/hello-world-framework": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts" : {
        "post-create-project-cmd": [
            "php setup.php"
        ]
    }
}